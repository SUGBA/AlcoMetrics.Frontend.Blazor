{
  //Параметры для Аутентификации/Авторизации
  "AuthSetting": {
    //Доменный путь до агрегирующего сервиса
    "AuthenticationServicePath": "https://localhost:5002",
    //Точка для авторизации
    "LoginPath": "Account/Login",
    //Ключ с именем пользователя
    "UserNameClaim": "name",
    //Ключ с ролями пользователя
    "UserRolesClaim": "roles",
    //Имя переменной в cookie для хранения токена
    "TokenVariableName": "token"
  }
  "ApiSettings": {
    //Имя HttpClient
    "HttpClientName": "Wine Analyzer Web User"
  }
}